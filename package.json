{
  "name": "secs-message-explorer",
  "displayName": "SECS Message Explorer",
  "description": "Explore secs command",
  "version": "0.0.1",
  "publisher": "ychu0",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCommand:extension.secs.explore.file",
    "onCommand:extension.secs.explore.folder",
    "onCommand:extension.secs.setting",
    "onView:secs-messages"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "secs-gem-expolorer",
          "title": "SECS/GEM Explorer",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "secs-gem-expolorer": [
        {
          "id": "secs-messages",
          "name": "Secs Messages"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.secs.explore.file",
        "title": "SECS/GEM Explorer: Explore File"
      },
      {
        "command": "extension.secs.explore.folder",
        "title": "SECS/GEM Explorer: Explore Workspace"
      },
      {
        "command": "extension.secs.setting",
        "title": "SECS/GEM Explorer: Setting"
      },
      {
        "command": "extension.gotoMessage",
        "title": "Edit"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "extension.gotoMessage",
          "when": "view == secs-messages && viewItem == TreeItem"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Secs Message Explorer",
      "properties": {
        "secs.hideUnusedS6F11": {
            "type": "boolean",
            "description": "是否隱藏無用的S6F11訊息？",
            "default": true
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "@types/ramda": "^0.26.9"
  }
}
